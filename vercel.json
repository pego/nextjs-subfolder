{
    "rewrites": [
        {
          "source": "/:path*",
          "has": [
            {
              "type": "host",
              "value": "nextjs-subfolder.vercel.app"
            }
          ],
          "destination": "https://nextjs-root.vercel.app/spring-summer-2023-fashion-show/:path*"
        }
      ],
    "trailingSlash": true
}